{
  "name": "Daf Yomi Helper",
  "version": "1.6",
  "manifest_version": 2,
  "description": "Daf Helper Plugin",
  "icons": {
    "16": "DafYomi_16x16.png",
    "48": "DafYomi_48x48.png",
    "128": "DafYomi_128x128.png"
  },
  "page_action": {
    "default_icon": {
      "16": "DafYomi_16x16.png",
      "48": "DafYomi_48x48.png",
      "128": "DafYomi_128x128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "*://daf-yomi.com/*",
        "*://*.daf-yomi.com/*"
      ],
      "js": [
        "jquery-1.8.2.min.js",
        "contentScript.js"
      ],
      "css": [
        "daf.css"
      ],
      "run_at": "document_idle"
    }
  ]
}